{
  "name": "php-fn/php-fn",
  "description": "a php library",
  "keywords": ["php", "library"],
  "homepage": "https://github.com/php-fn/php-fn",
  "license": "MIT",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "php-fn/test": "^1",
    "ext-simplexml": "*"
  },
  "autoload": {
    "psr-4": {
      "fn\\": ["src/fn"]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "fn\\": ["tests/fn"]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
