{
  "name": "php-fn/php-fn",
  "description": "a php library",
  "keywords": ["php", "library"],
  "homepage": "https://github.com/php-fn/php-fn",
  "license": "MIT",
  "authors": [
    {"name": "Demjan Kaluzki", "email": "phpfn.phpfn@gmail.com"}
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "fn\\": ["src/fn"]
    },
    "files": [
      "src/fn/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "fn\\": ["tests/fn"]
    },
    "files": [
      "tests/fn/functions.php"
    ]
  }
}
